<uk.ac.abdn.t3.t3v2app.CustomLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="core.OverviewActivity"
  
    tools:ignore="MergeRootFrame" 
>

    <FrameLayout android:id="@+id/description_frag"
        		android:layout_width="match_parent"
        		android:layout_height="wrap_content"        		
        		android:background="@color/white"/>
    
      <FrameLayout android:id="@+id/pic_frag"
        		android:layout_width="match_parent"
        		android:layout_height="wrap_content"
        		android:background="@color/white"/>
      
      
        <FrameLayout android:id="@+id/cap_frag"
        		android:layout_width="match_parent"
        		android:layout_height="0dip"
        		android:layout_weight="6"
        		/>
        
        <FrameLayout android:id="@+id/button_frag"
        		android:layout_width="match_parent"
        		android:layout_height="0dip"
        		android:layout_weight="1"
        		/>
        
         
   
    </uk.ac.abdn.t3.t3v2app.CustomLayout>
